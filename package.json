{
  "name": "@MJordanMilliman/milliman-design-system-test",
  "description": "Themes for applying color in the Carbon Design System",
  "version": "0.0.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "npm run compile-sass && npm run postcss && npm run minify-css",
    "compile-sass": "node-sass themes/scss --output themes/css --include-path node_modules/",
    "postcss": "postcss themes/css/white/index.css -r --use autoprefixer --no-map",
    "minify-css": "csso themes/css/white/index.css --output themes/css/white/index.min.css"
  },
  "dependencies": {
    "@carbon/themes": "^10.29.0",
    "autoprefixer": "^10.4.2",
    "carbon-components": "^10.20.0",
    "node-sass": "^4.14.1"
  },
  "devDependencies": {
    "csso-cli": "^3.0.0",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0"
  }
}
