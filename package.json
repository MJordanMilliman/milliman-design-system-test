{
  "name": "@MJordanMilliman/milliman-design-system-test",
  "description": "Themes for applying color in the Carbon Design System",
  "version": "0.3.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "publishConfig": {
    "registry":"https://npm.pkg.github.com"
  },
  "scripts": {
    "compile-sass": "node-sass themes/white/scss/index.scss themes/white/css/white.css --include-path node_modules/ --source-map themes/white/css/white.css.map",
    "minify-css": "csso themes/white/css/white.css --output themes/white/css/white.min.css --input-source-map themes/white/css/white.css.map --source-map themes/white/css/white.min.map"
  },
  "dependencies": {
    "carbon-components": "^10.20.0",
    "node-sass": "^4.14.1"
  },
  "devDependencies": {
    "csso-cli": "^3.0.0"
  }
}
